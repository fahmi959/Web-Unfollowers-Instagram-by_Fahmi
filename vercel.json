{
  "version": 2,
  "builds": [
    {
      "src": "api/routes/**/*.js",
      "use": "@vercel/node"
    },
    {
      "src": "public/**/*",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/api/v1/instagram",
      "dest": "/api/routes/instagram.js"
    },
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    }
  ]
}
